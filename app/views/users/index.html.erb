
<h1>List all users</h1>


<table>
  <thead>
    <tr>
      <th>id</th>
      <th>email</th>
      <th>created</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
